<div class="list-inner component-inner">
    {{> component this}}
    <div class="list-widget component-widget">
	    <div class="list-container{{#if _showBackground}} background-enabled{{/if}}">
		    {{#each _items}}
		    <div class="list-item item-{{@index}}">
          {{#if _imageSrc}}
          <div class="list-item-image">
            <img src="{{_imageSrc}}" {{#if alt}}aria-label="{{alt}}"{{else}}aria-hidden="true"{{/if}}/>
          </div>
          {{/if}}
		    	<div class="list-item-title">{{{compile title}}}</div>
		    </div>
		    {{/each}}
	    </div>
	</div>
</div>
